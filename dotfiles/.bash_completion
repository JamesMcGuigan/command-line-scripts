#!/usr/bin/env bash

# Paperspace API Key: https://docs.paperspace.com/gradient/get-started/install-the-cli#obtaining-an-api-key
if [[ `which gradient 2> /dev/null` ]]; then  eval "$(_GRADIENT_COMPLETE=source gradient)";  fi

# Node Version Manager: https://github.com/nvm-sh/nvm/blob/master/README.md
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion
